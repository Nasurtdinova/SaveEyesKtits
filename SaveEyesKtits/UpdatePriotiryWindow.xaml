        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SaveEyesKtits"
        mc:Ignorable="d"
        Title="UpdatePriotiryWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock>Значение</TextBlock>
            <TextBox PreviewTextInput="tbValue_PreviewTextInput" x:Name="tbValue"></TextBox>
            <Button x:Name="btnOk" Click="btnOk_Click">Изменить</Button>
        </StackPanel>
    </Grid>
</Window>
